{
    "#visibility-window": {
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "5"
        }
    },
    "#visibility-window.#title_bar": {
        "font": {
            "name": "CascadiaCode",
            "size": "16"
        }
    },
    "#visibility-window.#close_button": {
        "colours": {
            "normal_bg": "#B02828",
            "hovered_bg": "#A33636",
            "active_bg": "#6E1B1B",
            "active_border": "#73787B"
        }
    },
    "#visibility-drop-down": {
        "misc": {
            "list_item_height": "40"
        },
        "colours": {
            "normal_border": "#313338",
            "dark_bg": "#313338"
        }
    },
    "#visibility-drop-down.@selection_list_item": {
        "colours": {
            "normal_text": "#B5B2B4",
            "normal_bg": "#313338"
        },
        "misc": {
            "border_width": "3",
            "text_horiz_alignment": "center",
            "text_vert_alignment": "top",
            "text_horiz_alignment_padding": "10",
            "text_vert_alignment_padding": "5"
        },
        "font": {
            "name": "CascadiaCode",
            "size": "16"
        }
    },
    "#theme-selection-drop-down": {
        "misc": {
            "open_button_width": "35",
            "expand_direction": "up",
            "shape": "rounded_rectangle"
        }
    },
    "#theme-selection-drop-down.#selected_option": {
        "misc": {
            "border_width": "2",
            "text_horiz_alignment": "center",
            "text_vert_alignment": "top",
            "text_horiz_alignment_padding": "10",
            "text_vert_alignment_padding": "8"
        },
        "colours": {
            "normal_text": "#B5B2B4",
            "normal_bg": "#313338"
        },
        "font": {
            "name": "CascadiaCode",
            "size": "16"
        }
    },
    "#theme-selection-drop-down.#drop_down_options_list": {
        "misc": {
            "list_item_height": "40",
            "shape": "rounded_rectangle",
            "shape_corner_radius": "5"
        }
    },
    "#theme-selection-drop-down.#drop_down_options_list.#vertical_scroll_bar": {
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "5"
        }
    },
    "#theme-selection-drop-down.#drop_down_options_list.vertical_scroll_bar.#top_button": {
        "misc": {
            "border_width": "2"
        },
        "colours": {
            "normal_text": "#B5B2B4",
            "normal_bg": "#313338"
        }
    },
    "#theme-selection-drop-down.#drop_down_options_list.vertical_scroll_bar.#bottom_button": {
        "misc": {
            "border_width": "2"
        },
        "colours": {
            "normal_text": "#B5B2B4",
            "normal_bg": "#313338"
        }
    },
    "#theme-selection-drop-down.#drop_down_options_list.button": {
        "font": {
            "name": "CascadiaCode",
            "size": "14"
        },
        "misc": {
            "shape_corner_radius": "5",
            "border_width": "3",
            "text_horiz_alignment": "center",
            "text_vert_alignment": "top",
            "text_horiz_alignment_padding": "10",
            "text_vert_alignment_padding": "5"
        },
        "colours": {
            "normal_text": "#B5B2B4",
            "normal_bg": "#313338"
        }
    },
    "#theme-selection-drop-down.#expand_button": {
        "misc": {
            "border_width": "2"
        },
        "colours": {
            "normal_text": "#B5B2B4",
            "normal_bg": "#313338"
        }
    },
    "#command-textbox": {
        "font": {
            "name": "CascadiaCode",
            "size": "16"
        },
        "colours": {
            "dark_bg": "#313338",
            "normal_text": "#B5B2B4"
        },
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "5",
            "border_width": "3",
            "padding": "7, 7"
        }
    },
    "#message-textbox": {
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "5",
            "border_width": "3",
            "padding": "13, 7"
        },
        "colours": {
            "dark_bg": "#313338"
        }
    },
    "Message Box Content Palette": {
        "normal_text": "#B5B2B4",
        "horizontal_line": "#808080",
        "error_text": "#FC0A3D",
        "output_text": "#00FF00",
        "command": "#61AFEF",
        "argument_name": "#E5C17A",
        "optional_notation": "#56B7C3",
        "colon": "#B5B2B4",
        "type": "#56B7C3"
    }
}